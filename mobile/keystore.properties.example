# ===========================================
# Keystore Configuration Template
# ===========================================
# Copy this file to keystore.properties and fill in your values
# DO NOT commit keystore.properties to version control!

# Release keystore configuration
RELEASE_STORE_FILE=path/to/your/release-keystore.jks
RELEASE_STORE_PASSWORD=your_store_password
RELEASE_KEY_ALIAS=your_key_alias
RELEASE_KEY_PASSWORD=your_key_password

# ===========================================
# How to create a release keystore:
# ===========================================
# keytool -genkeypair -v \
#   -keystore liyaqa-release.jks \
#   -keyalg RSA \
#   -keysize 2048 \
#   -validity 10000 \
#   -alias liyaqa \
#   -dname "CN=Liyaqa, OU=Mobile, O=Liyaqa Ltd, L=Riyadh, ST=Riyadh, C=SA"
#
# Then update this file with:
# RELEASE_STORE_FILE=liyaqa-release.jks
# RELEASE_STORE_PASSWORD=<password you set>
# RELEASE_KEY_ALIAS=liyaqa
# RELEASE_KEY_PASSWORD=<password you set>
