<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- ===========================================
         Export Options for App Store Distribution
         =========================================== -->

    <!-- Export Method: app-store, ad-hoc, enterprise, development -->
    <key>method</key>
    <string>app-store</string>

    <!-- Team ID from Apple Developer Account -->
    <key>teamID</key>
    <string>YOUR_TEAM_ID</string>

    <!-- Upload to App Store Connect -->
    <key>uploadBitcode</key>
    <true/>

    <!-- Compile bitcode for store -->
    <key>compileBitcode</key>
    <true/>

    <!-- Upload symbols for crash reporting -->
    <key>uploadSymbols</key>
    <true/>

    <!-- Signing Style: automatic or manual -->
    <key>signingStyle</key>
    <string>automatic</string>

    <!-- For manual signing, specify provisioning profiles -->
    <!--
    <key>provisioningProfiles</key>
    <dict>
        <key>com.liyaqa.member</key>
        <string>Liyaqa Member App Store</string>
    </dict>
    -->

    <!-- Strip Swift symbols to reduce size -->
    <key>stripSwiftSymbols</key>
    <true/>

    <!-- Destination: export for App Store Connect upload -->
    <key>destination</key>
    <string>upload</string>

    <!-- Generate app store info -->
    <key>generateAppStoreInformation</key>
    <false/>

    <!-- Thinning: none, thin-for-all-variants, or specific device -->
    <key>thinning</key>
    <string>&lt;none&gt;</string>

    <!-- Include on-demand resources -->
    <key>embedOnDemandResourcesAssetPacksInBundle</key>
    <false/>

    <!-- iCloud container environment -->
    <key>iCloudContainerEnvironment</key>
    <string>Production</string>

    <!-- Manage App Version and Build Number -->
    <key>manageAppVersionAndBuildNumber</key>
    <true/>
</dict>
</plist>
